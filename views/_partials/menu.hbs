  <div class="newspaper-main-menu" id="stickyMenu">
            <div class="classy-nav-container breakpoint-off">
                <div class="container">
                    <!-- Menu -->
                    <nav class="classy-navbar justify-content-between" id="newspaperNav">
    
                        <!-- Logo -->
                        <div class="logo">
                            <a href="/"><img src="/public/images/core-img/logo-3.png" alt=""></a>
                        </div>
                        <!-- Navbar Toggler -->
                        <div class="classy-navbar-toggler">
                            <span class="navbarToggler"><span></span><span></span><span></span></span>
                        </div>
                        <!-- Menu -->
                        <div class="classy-menu d-flex" style="margin-left: 25px;">
    
                            <!-- close btn -->
                            <div class="classycloseIcon">
                                <div class="cross-wrap"><span class="top"></span><span class="bottom"></span></div>
                            </div>
    
                            <!-- Nav Start -->
                            <div class="classynav" style="display: block !important;">
                                <ul class="justify-content-around">
                                    {{#if this.empty_cateParent}}
                                    {{else}}
                                    {{#each this.list_cateParent}}
                                    <li><a href="#">{{name}}</a>
                                        <ul class="dropdown" id="abc">
                                            {{#each this.list_cateChild}}
                                            <li><a href="/ListArt/{{id}}">{{name}}</a></li>
                                            {{/each}}
                                        </ul>
                                    </li>
                                    {{/each}}
                                    {{/if}}
                                    <li><a href="#"><i class="fa fa-ellipsis-h fa-lg "></i></a>
                                        <div class="megamenu">
                                            {{#each this.list_cateParentAll}}
                                            <ul class="single-mega cn-col-4">
                                               <li class="title"><a class="title" href="/ListArt/{{id}}">{{name}}</a></li>
                                                {{#each this.list_cateChild}}
                                                <li><a href="/ListArt/{{id}}">{{name}}</a></li>
                                                {{/each}}
                                            </ul>
                                            {{/each}}
                                        </div>
                                    </li>
                                </ul>
                            </div>
                            <!-- Nav End -->
                        </div>
                    </nav>
                </div>
            </div>
        </div>